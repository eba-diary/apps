<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="sentrySettings">
      <section name="EnvironmentSettings" type="Sentry.Configuration.EnvironmentSettingsSectionHandler,Sentry.Configuration" />
    </sectionGroup>
  </configSections>
  <!-- log4net settings -->
  <log4net threshold="ALL">
    <logger name="Sentry.LoggerDev">
      <level value="DEBUG" />
      <appender-ref ref="RollingLogFileAppenderLocal" />
      <appender-ref ref="SELAppenderLocal" />
    </logger>
    <logger name="Sentry.LoggerTest">
      <level value="DEBUG" />
      <appender-ref ref="RollingLogFileAppenderServer" />
      <appender-ref ref="SELAppenderServer" />
    </logger>
    <logger name="Sentry.LoggerQual">
      <level value="INFO" />
      <appender-ref ref="RollingLogFileAppenderServer" />
    </logger>
    <logger name="Sentry.LoggerProd">
      <level value="INFO" />
      <appender-ref ref="RollingLogFileAppenderServer" />
    </logger>
    <root>
      <level value="OFF" />
    </root>
    <appender name="RollingLogFileAppenderLocal" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString">
        <conversionPattern value="C:\\Temp\\Applogs\\Sentry.data.Bundler.WindowsService.log" />
      </file>
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="20MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-23date [%-5thread] %-5level %message%newline" />
      </layout>
    </appender>
    <appender name="SELAppenderLocal" type="Sentry.Common.LogAppenders.NonWebApplicationAppender, Sentry.Common">
      <file value="This is not used" />
      <applicationName value="DatasetBundler" />
      <logFileName type="log4net.Util.PatternString" value="DatasetBundler.log" />
      <logFolderFilePath value="C:\\Temp\\AppLogs\\DatasetBundler\\" />
      <!-- Use this environmentName value if you are using Sentry.Configuration and have the 2.1.6 version-->
      <environmentName value="Sentry.Configuration.EnvironmentName, Sentry.Configuration" />
    </appender>
    <appender name="RollingLogFileAppenderServer" type="log4net.Appender.RollingFileAppender">
      <file value="${LOGPATH}\BundleService.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="20MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-23date [%-5thread] %-5level %message%newline" />
      </layout>
    </appender>
    <appender name="SELAppenderServer" type="Sentry.Common.LogAppenders.NonWebApplicationAppender, Sentry.Common">
      <file value="This is not used" />
      <applicationName value="DatasetBundler" />
      <logFileName type="log4net.Util.PatternString" value="DatasetBundler.log" />
      <logFolderFilePath value="${LOGPATH}" />
      <!-- Use this environmentName value if you are using Sentry.Configuration and have the 2.1.6 version-->
      <environmentName value="Sentry.Configuration.EnvironmentName, Sentry.Configuration" />
    </appender>
  </log4net>
  <!-- Sentry.Configuration settings -->
  <sentrySettings>
    <EnvironmentSettings defaultDomain="sentry.com" xmlns="urn:Sentry.Configuration">
      <!-- If you have a shared "defaultSettings" config file, import it here -->
      <!-- <importSettings path="defaultSettings.config" /> -->
      <defaultEnvironmentList>
        <env name="DEV">
          <machine nameRegex="(sho|dix|mad|ste)(is|it).*" />
          <machine executablePathRegex=".*Mainline-NR.*" />
        </env>
        <env name="TEST">
          <machine nameRegex="(sho|dix)-t-.*" />
        </env>
        <env name="QUAL">
          <machine nameRegex="(sho|dix)-q-.*" />
        </env>
        <env name="PROD">
          <machine nameRegex="(sho|dix)-p-.*" />
        </env>
      </defaultEnvironmentList>
      <config key="AppLogger">
        <settingList>
          <setting name="DEV" value="Sentry.LoggerDev" />
          <setting name="TEST" value="Sentry.LoggerTest" />
          <setting name="QUAL" value="Sentry.LoggerQual" />
          <setting name="PROD" value="Sentry.LoggerProd" />
        </settingList>
      </config>
      <config key="BundleRequestDir">
        <settingList>
          <setting name="DEV" value="C:\tmp\DatasetBundle\request\" />
          <setting name="TEST" value="\\sentry.com\appfs_nonprod\DatasetBundle_Test\request\" />
          <setting name="NR" value="\\sentry.com\appfs_nonprod\DatasetBundle_NR\request\" />
          <setting name="QUAL" value="\\sentry.com\appfs_nonprod\DatasetBundle_Qual\request\" />
          <setting name="PROD" value="\\sentry.com\appfs\DatasetBundle\request\" />
        </settingList>
      </config>
      <config key="BundleBaseWorkDirectory">
        <settingList>
          <setting name="DEV" value="C:\tmp\DatasetBundle\work\" />
          <!--<setting name="TEST" value="\\sentry.com\appfs_nonprod\DatasetBundle_Test\work\" />-->
          <setting name="TEST" value="%WORKINGSTORAGE%\work\"/>
          <!--<setting name="NR" value="\\sentry.com\appfs_nonprod\DatasetBundle_NR\work\"/>
          <setting name="Qual" value="D:\\AppIO\DATA-QUAL-BundleService\work\"/>
          <setting name="Prod" value="D:\\AppIO\DATA-PROD-BundleService\work\"/>-->
        </settingList>
      </config>
      <config key="SentryS3ProxyHost">
        <settingList>
          <setting name="DEV" value="awe-n-s3proxy.sentry.com" />
          <setting name="TEST,Test-NR" value="awe-n-s3proxy.sentry.com" />
          <setting name="QUAL" value="awe-n-s3proxy.sentry.com" />
          <setting name="PROD" value="awe-p-s3proxy.sentry.com" />
        </settingList>
      </config>
      <config key="SentryWebProxyHost">
        <settingList>
          <setting name="DEV" value="webproxy.sentry.com" />
          <setting name="TEST,Test-NR" value="serverproxyqual.sentry.com" />
          <setting name="QUAL" value="serverproxyqual.sentry.com" />
          <setting name="PROD" value="serverproxyprod.sentry.com" />
        </settingList>
      </config>
      <config key="AWSRootBucket">
        <settingList>
          <setting name="DEV,TEST,Test-NR,QUAL" value="sentry-dataset-management-np" />
          <setting name="PROD" value="sentry-dataset-management" />
        </settingList>
      </config>
      <config key="S3DataPrefix">
        <settingList>
          <setting name="DEV" value="data-dev/" />
          <setting name="TEST" value="data-test/" />
          <setting name="Test-NR" value="data-testnr/" />
          <setting name="QUAL,PROD" value="data/" />
        </settingList>
      </config>
      <config key="AWSAccessKey">
        <settingList>
          <setting name="DEV" externalValueFile="C:\Temp\IIS_SECURED\Data\Dev_Settings.xml" />
          <setting name="TEST" externalValueFile="D:\SecureSettings\DATA-TEST-BundleService\Settings.xml" />
          <setting name="QUAL,PROD" externalValueFile="%SecretFile%\Settings.xml" />
        </settingList>
      </config>
      <config key="AWSSecretKey">
        <settingList>
          <setting name="DEV" externalValueFile="C:\Temp\IIS_SECURED\Data\Dev_Settings.xml" />
          <setting name="TEST" externalValueFile="D:\SecureSettings\DATA-TEST-BundleService\Settings.xml" />
          <setting name="QUAL,PROD" externalValueFile="%SecretFile%\Settings.xml" />
        </settingList>
      </config>
      <config key="DatabaseConnectionString">
        <settingList>
          <setting name="DEV" value="Data Source=DIX-D-SHDDBS-02.sentry.com;Initial Catalog=SentryDatasets;Integrated Security=SSPI;" />
          <!--<setting name="Dev-NR" value="Data Source=DIX-D-SHDDBS-02.sentry.com;Initial Catalog=SentryDatasets-NR;Integrated Security=SSPI;" />-->
          <setting name="TEST" value="Data Source=DIX-T-SHDDBS-02.sentry.com;Initial Catalog=SentryDatasets;Integrated Security=SSPI;" />
          <!--<setting name="Test-NR" value="Data Source=DIX-T-SHDDBS-02.sentry.com;Initial Catalog=SentryDatasets-NR;Integrated Security=SSPI;" />-->
          <setting name="QUAL" value="Data Source=DIX-Q-SHDDBS-02;Initial Catalog=SentryDatasets;Integrated Security=SSPI;" />
          <setting name="PROD" value="Data Source=SHO-P-SHDDBS-02;Initial Catalog=SentryDatasets;Integrated Security=SSPI;" />
        </settingList>
      </config>
      <config key="ObsidianUserCacheTimeoutMinutes">
        <settingList>
          <setting name="DEV,TEST,QUAL" value="1" />
          <setting name="PROD" value="10" />
        </settingList>
      </config>
      <config key="WebApiUrl">
        <settingList>
          <setting name="DEV" value="http://localhost:2457" />
          <setting name="TEST" value="http://datatest.sentry.com" />
          <setting name="QUAL" value="http://dataqual.sentry.com" />
          <setting name="PROD" value="http://data.sentry.com" />
        </settingList>
      </config>
      <config key="LoaderRequestPath">
        <settingList>
          <setting name="Dev,NR_D" value="C:\tmp\DatasetLoaderRequest\" />
          <setting name="Test" value="\\sentry.com\appfs_nonprod\DatasetLoaderRequest_Test\" />
          <setting name="Qual" value="\\sentry.com\appfs_nonprod\DatasetLoaderRequest\" />
          <setting name="Prod" value="\\sentry.com\appfs\DatasetLoaderRequest\" />
        </settingList>
      </config>
    </EnvironmentSettings>
  </sentrySettings>
  <appSettings>
    <add key="AWSRegion" value="us-east-1" />
    <add key="SentryWebProxyPort" value="80" />
    <add key="SentryS3ProxyPort" value="80" />
    <add key="S3PreviewPrefix" value="Previews/" />
    <add key="ApplicationName" value="DatasetBundler" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="log4net" publicKeyToken="669e0ddf0bb1aa2a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.2.15.0" newVersion="1.2.15.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="NHibernate" publicKeyToken="aa95f207798dfdb4" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.0.4000" newVersion="4.0.0.4000" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="MiniProfiler" publicKeyToken="b44f9351044011a3" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.2.0.157" newVersion="3.2.0.157" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>