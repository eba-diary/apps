<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="sentrySettings">
      <section name="EnvironmentSettings" type="Sentry.Configuration.EnvironmentSettingsSectionHandler,Sentry.Configuration" />
    </sectionGroup>
  </configSections>
  <!-- log4net settings -->
  <log4net threshold="ALL">
    <logger name="Sentry.LoggerDev">
      <level value="DEBUG" />
      <appender-ref ref="RollingLogFileAppenderLocal" />
      <appender-ref ref="SELAppenderLocal" />
    </logger>
    <logger name="Sentry.LoggerTest">
      <level value="DEBUG" />
      <appender-ref ref="RollingLogFileAppenderServer" />
      <appender-ref ref="SELAppenderServer" />
    </logger>
    <logger name="Sentry.LoggerNRTest">
      <level value="DEBUG" />
      <appender-ref ref="RollingLogFileAppenderServerNRTest" />
      <appender-ref ref="SELAppenderServerNRTest" />
    </logger>
    <logger name="Sentry.LoggerQual">
      <level value="INFO" />
      <appender-ref ref="RollingLogFileAppenderServer" />
      <appender-ref ref="SELAppenderServer" />
    </logger>
    <logger name="Sentry.LoggerProd">
      <level value="INFO" />
      <appender-ref ref="RollingLogFileAppenderServer" />
      <appender-ref ref="SELAppenderServer" />
    </logger>
    <root>
      <level value="OFF" />
    </root>
    <appender name="RollingLogFileAppenderLocal" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString">
        <conversionPattern value="C:\\Temp\\Applogs\\Goldeneye.WindowsService.log" />
      </file>
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="20MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-23date [%-5thread] %-5level %message%newline" />
      </layout>
    </appender>
    <appender name="SELAppenderLocal" type="Sentry.Common.LogAppenders.NonWebApplicationAppender, Sentry.Common">
      <file value="This is not used" />
      <applicationName value="GoldenEye" />
      <logFileName type="log4net.Util.PatternString" value="GoldenEye.log" />
      <logFolderFilePath value="C:\\Temp\\AppLogs\\GoldenEye\\" />
      <!-- Use this environmentName value if you are using Sentry.Configuration and have the 2.1.6 version-->
      <environmentName value="Sentry.Configuration.EnvironmentName, Sentry.Configuration" />
    </appender>
    <appender name="RollingLogFileAppenderServer" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString">
        <conversionPattern value="D:\\AppLogs\\Goldeneye\\Goldeneye.WindowsService.log" />
      </file>
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="20MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-23date [%-5thread] %-5level %message%newline" />
      </layout>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <appender name="RollingLogFileAppenderServerNRTest" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString">
        <conversionPattern value="D:\\AppLogs\\GoldeneyeNR\\Goldeneye.WindowsService.log" />
      </file>
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="20MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-23date [%-5thread] %-5level %message%newline" />
      </layout>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <appender name="SELAppenderServer" type="Sentry.Common.LogAppenders.NonWebApplicationAppender, Sentry.Common">
      <file value="This is not used" />
      <applicationName value="Goldeneye" />
      <logFileName value="GoldeneyeLogFile.log" />
      <logFolderFilePath value="D:\\AppLogs\\Goldeneye\\" />
      <!-- Use this environmentName value if you are using Sentry.Configuration and have the 2.1.6 version-->
      <environmentName value="Sentry.Configuration.EnvironmentName, Sentry.Configuration" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <appender name="SELAppenderServerNRTest" type="Sentry.Common.LogAppenders.NonWebApplicationAppender, Sentry.Common">
      <file value="This is not used" />
      <applicationName value="Goldeneye" />
      <logFileName value="GoldeneyeLogFile.log" />
      <logFolderFilePath value="D:\\AppLogs\\GoldeneyeNR\\" />
      <!-- Use this environmentName value if you are using Sentry.Configuration and have the 2.1.6 version-->
      <environmentName value="Sentry.Configuration.EnvironmentName, Sentry.Configuration" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
  </log4net>
  <!-- Sentry.Configuration settings -->
  <sentrySettings>
    <EnvironmentSettings defaultDomain="sentry.com">
      <defaultEnvironmentList>
        <env name="Dev">
          <!--<webHost name="."/>-->
          <machine executablePathRegex=".*Mainline[^-].*" />
          <machine nameRegex="(sho|dix|mad|ste)(is|it).*"/>
        </env>
        <!--<env name="NR_D">
          --><!--<webHost name="localhost" />--><!--
          <machine nameRegex="(sho|dix|mad|ste)(is|it).*" />
          <machine executablePathRegex=".*Mainline-NR.*" />
        </env>-->
        <env name="Test">
          <!--<webHost nameRegex="DatasetLoadertest.?" />-->
          <machine nameRegex="(sho|dix)-t-.*" />
          <machine executablePathRegex=".*[^-][^N][^R]Test\.*" />
        </env>
        <!--<env name="NR_T">
          --><!--<webHost nameRegex="DatasetLoadertest.?" />--><!--
          <machine nameRegex="(sho|dix)-t-.*" />
          <machine executablePathRegex=".*NRTest\.*" />
        </env>-->
        <env name="Qual">
