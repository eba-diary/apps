<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections xdt:Transform="InsertIfMissing" >
    <sectionGroup name="sentrySettings" xdt:Transform="InsertIfMissing" xdt:Locator="Match(name)">
      <section name="EnvironmentSettings" type="Sentry.Configuration.EnvironmentSettingsSectionHandler,Sentry.Configuration" xdt:Transform="InsertIfMissing" xdt:Locator="Match(name)"/>
    </sectionGroup>
  </configSections>
  
  <!-- Transform below for those with a sentrySettings\EnvironmentSettings sections -->
  <sentrySettings xdt:Transform="InsertIfMissing">
    <!-- NOTE: Use Ctrl+K Ctrl+D to reformat your config file to make the following readable -->
    <EnvironmentSettings defaultDomain="sentry.com" xmlns="urn:Sentry.Configuration"
      xdt:Transform="InsertIfMissing">
      <defaultEnvironmentList>
        <env name="Dev">
          <webHost name="localhost"/>
          <machine nameRegex="(sho|dix|mad)(is|it).*"/>
        </env>
        <env name="Test">
          <webHost nameRegex="_MyApp_test.?"/>
          <machine nameRegex="(sho|dix)-t-.*"/>
        </env>
        <env name="Qual">
          <webHost nameRegex="_MyApp_qual.?"/>
          <machine nameRegex="(sho|dix)-q-.*"/>
        </env>
        <env name="Prod">
          <webHost nameRegex="_MyApp_.?"/>
          <machine nameRegex="(sho|dix)-p-.*"/>
        </env>
      </defaultEnvironmentList>
      <config key="EnvironmentName">
        <settingList>
          <setting name="Dev" value="Dev"/>
          <setting name="Test" value="Test"/>
          <setting name="Qual" value="Qual"/>
          <setting name="Prod" value="Prod"/>
        </settingList>
      </config>
      <!-- Add other config keys as needed -->
    </EnvironmentSettings>
  </sentrySettings>
  
</configuration>
