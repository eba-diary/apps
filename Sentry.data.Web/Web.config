    <SentryPrincipal ApplicationName="DatasetManagement" AdSecUrlKey="AdSecUrl" />
    <!--
    RegistrationURL is an option to have the user register.
            LoginUrlKey="LoginURL" - Create a cookie, that will be the single sign on token
            AuthenticationServiceUrlKey="authServiceUrl" - called to validate the token.
            AuthenticationServiceProfileKey="authServiceProfile" - Way to determine what kind of authentication you want to use, default, extranet, intranet - more to come.
            SupressLoginRedirectsForAjaxCalls="True" - Doesn't have an associated key in default settings. Heavy AJAX, want this to be true. JSON would be the html of the redirect page if false.                              
            ProfileUrlKey="profileUrl" - Used for getting information about people in the login service. Name contact info.
            ProfileCredentialsUserNameKey="ServiceAccountID" - ID and Password, in secured file.
            ProfileCredentialsPasswordKey="ServiceAccountPassword"
            RequestingApplicationKey="RequestingApplication" - Used for tracking what application is calling the login service. - Talk to Mike Heier when we get to user management.
            SSOCookieDomain=".sentry.com"  - For future use.     
    -->
    <!--<SentryLoginSettings LoginUrlKey="LoginURL"
                     AuthenticationServiceUrlKey="authServiceUrl"
                     AuthenticationServiceProfile="Internal"
                     SupressLoginRedirectsForAjaxCalls="True"
                     SSOCookieDomain=".sentry.com" />-->    
    <SmartsSettings serviceName="data" xmlns="urn:Sentry.Smarts.Configuration">
