        //https://stackoverflow.com/questions/1250079/how-to-escape-single-quotes-within-single-quoted-strings
        String.prototype.escapeForJson = function () {
            return this
                .replace(/\b/g, "")
                .replace(/\f/g, "")
                .replace(/\\/g, "\\")
                .replace(/\"/g, "\\\"")
                .replace(/\t/g, "\\t")
                .replace(/\r/g, "\\r")
                .replace(/\n/g, "\\n")
                .replace(/\u2028/g, "\\u2028")
                .replace(/\u2029/g, "\\u2029");
        };

