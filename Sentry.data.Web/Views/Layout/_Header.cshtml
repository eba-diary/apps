@model HeaderModel

<head>
    <title>Data at Sentry</title> <!-- Fallback title -->
</head>

<div id="internetExplorerWarning" role="banner" aria-hidden="false" style="padding:10px; font-size: 14px; display:none;" class="alert alert-warning">
    <span class="fas fa-exclamation-circle" style="margin: 0px 15px 0px 5px;"></span>
    We noticed you are using <strong>Internet Explorer</strong>.  Many of our site's functionalities use features only available in <strong>
        Edge, Firefox, or Chrome.
    </strong>  We recommend you switch to one of those browsers. <a href="microsoft-edge:@Request.Url.AbsoluteUri">Click here to change to Edge.</a>
</div>

<div class="sticky-top">
    <nav class="sentry-header navbar navbar-light" style="border: none;">
        <a class="navbar-brand" href="/">
            <img class="sentry-logo" src="~/Images/logo_blue.svg" alt="">
        </a>
        <div class="sentry-app-title">
            <h1>data.sentry.com</h1>
            <p> &nbsp;</p>
        </div>



        <div class="right-buttons">
            <div class="dropdown sentry-user-dropdown">
                <button class="dropdown-toggle sentry-user btn btn-primary waves-effect waves-light" data-toggle="dropdown">
                    @{ if (Model.IsImpersonating)
                        {
                            <span class="icon-account"></span>

                        }
                        else
                        {
                            <span><img src="@Model.AssociatePhotoUrl" /></span>
                        }

                    }
                    <span class="sentry-user-name">@Model.CurrentUserName</span>
                </button>
                @if (Model.IsImpersonating || Model.CanUserSwitch)
                {
                    <div class="dropdown-menu dropdown-menu-right" role="menu">
                        @{if (Model.IsImpersonating)
                            {
                                <a class="dropdown-item" tabindex="-1" id="restoreUser" href="#">Switch back to @Model.RealUserName</a>
                            }
                        }
                        @{if (Model.CanUserSwitch)
                            {
                                <a id="dscSession" class="dropdown-item" tabindex="-1" href="">Primary DSC Livy Session</a>
                                <a class="dropdown-item" tabindex="-1" href="@Url.Action("Switch", "User")">User Switch to Anybody</a>
                                <div class="dropdown-divider"></div>
                                <div class="dropdown-header">Quick User Switch</div>
                                <a class="dropdown-item" tabindex="-1" href="@Url.Action("RequestSwitch", "User", new { id = "070557" })">Manage Asset Alerts</a>
                                <a class="dropdown-item" tabindex="-1" href="@Url.Action("RequestSwitch", "User", new { id = "081082" })">Inside IT, No Access</a>
                                <a class="dropdown-item" tabindex="-1" href="@Url.Action("RequestSwitch", "User", new { id = "082728" })">Outside IT, No Access</a>
                            }
                        }
                    </div>
                }
            </div>

            @{if (Model.EnvironmentName.ToUpper() != "PROD")
                {
                    <button class="sentry-environment btn btn-danger waves-effect waves-light">
                        <span class="sentry-environment-name">@Model.EnvironmentName.ToUpper()</span>
                    </button>
                }
            }

        </div>
    </nav>

    <nav class="navbar navbar-expand-lg navbar-dark sentry-dark-blue">
        <!-- Collapse button -->
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <!-- Collapsible content -->
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">

                <li class="nav-item dropdown multi-level-dropdown">

                    <a href="#" id="menu" data-toggle="dropdown" class="nav-link dropdown-toggle">Business Intelligence</a>
                    <ul class="dropdown-menu z-depth-1">
                        <li class="dropdown-item"><a class="" href="~/BusinessIntelligence/Index">Home</a></li>

                        <li class="dropdown-submenu">
                            <a href="#" data-toggle="dropdown" class="dropdown-toggle">Search</a>
                            <ul class="dropdown-menu z-depth-1">
                                <li class="dropdown-item">
                                    <a href="/Search/BusinessIntelligence/Index?searchPhrase=" class="waves-effect waves-light">All</a>
                                </li>
                                @foreach (Sentry.data.Web.Models.BusinessIntelligenceUIModel da in ViewBag.BusinessIntelligenceCategories)
                                {
                                    <li class="dropdown-item">
                                        <a href="/Search/BusinessIntelligence/Index?category=@da.Name" class="waves-effect waves-light">@da.Name</a>
                                    </li>
                                }

                            </ul>
                        </li>

                        <li><div class="dropdown-header">Exhibit Actions</div></li>
                        <li class="dropdown-item"><a class="" href="~/BusinessIntelligence/Create">Create Exhibit</a></li>

                    </ul>
                </li>



                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle waves-effect waves-light" id="navbarDropdownDatasetLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Datasets</a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdownDatasetLink">
                        <a class="dropdown-item" href="~/Dataset/Index">Home</a>
                        <a class="dropdown-item" href="~/Search/Datasets">Search</a>
                        <div class="dropdown-divider"></div>
                        <div class="dropdown-header">Dataset Actions</div>
                        @if (Model.DisplayDataflowMetadata)
                        {
                            <a class="dropdown-item waves-effect waves-light" href="/DataFlow/Create">Send Data to DSC</a>
                        }
                        else
                        {
                            <a class="dropdown-item waves-effect waves-light" href="/Dataset/Create">Create Dataset</a>
                        }
                        <div class="dropdown-divider"></div>
                        <div class="dropdown-header">Dataset Administration</div>
                        <a class="dropdown-item waves-effect waves-light" href="/Config/Source/Create">Create Retriever Source</a>
                    </div>
                </li>

