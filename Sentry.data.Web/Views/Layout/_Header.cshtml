@model HeaderModel



@if (!Model.CanUseApp)
{
    <div id="NoAccessWarning" role="banner" aria-hidden="false" style="margin-bottom: -2px; font-size: 14px;" class="alert alert-warning">
        <span class="glyphicon glyphicon-warning-sign" style="margin: 0px 15px 0px 5px;"></span>
        You currently <b>Do Not</b> have access to data.sentry.com.    Please <a target="_blank" href="http://sharepoint.sentry.com/it/DataArch/Shared%20Documents/Data.Sentry.com/099.%20DSC%20Site%20Documents/SiteAccess.docx?Web=1">click here</a> or go to our help menu above to find out how to get access through <a target="_blank" href="https://myaccess.sentry.com">My Access</a>.
    </div>

    <div id="internetExplorerWarning" role="banner" aria-hidden="false" style="font-size: 14px; display:none;" class="alert alert-warning">
        <span class="glyphicon glyphicon-warning-sign" style="margin: 0px 15px 0px 5px;"></span>
        We noticed you are using <strong>Internet Explorer</strong>.  Many of our site's functionalities use features only available in <strong>
            Edge, Firefox, or Chrome.
        </strong>  We recommend you switch to one of those browsers. <a href="microsoft-edge:@Request.Url.AbsoluteUri">Click here to change to Edge.</a>
    </div>
}
else
{
    <div id="internetExplorerWarning" role="banner" aria-hidden="false" style="padding:10px; font-size: 14px; display:none;" class="alert alert-warning">
        <span class="glyphicon glyphicon-warning-sign" style="margin: 0px 15px 0px 5px;"></span>
        We noticed you are using <strong>Internet Explorer</strong>.  Many of our site's functionalities use features only available in <strong>
            Edge, Firefox, or Chrome.
        </strong>  We recommend you switch to one of those browsers. <a href="microsoft-edge:@Request.Url.AbsoluteUri">Click here to change to Edge.</a>
    </div>
}

<nav class="navbar navbar-default navbar-fixed-top sentry-navbar" role="navigation">
    <header class="sentry-header">

        <a href="/"><div class="sentry-logo"></div></a>
        <div class="sentry-app-title">
            <h1>
                data.sentry.com
            </h1>
            @*<h2>
                    Data for all of Sentry
                </h2>*@
        </div>
        @{if (Model.EnvironmentName.ToUpper() != "PROD")
            {
                <div class="sentry-environment">
                    <span class="sentry-environment-name">@Model.EnvironmentName.ToUpper()</span>
                </div>
            }
        }
        <!-- User Menu -->
        <div class="dropdown sentry-user-dropdown">
            <div class="dropdown-toggle sentry-user" data-toggle="dropdown">
                @{if (Model.IsImpersonating)
                    {
                        <span class="glyphicon glyphicon-user text-info"></span>
                    }
                    else
                    {
                        //<span class="glyphicon glyphicon-user"></span>
                        <span><img src="@Model.AssociatePhotoUrl" /></span>
                    }
                }
                <span class="sentry-user-name">@Model.CurrentUserName</span>
            </div>
            @if (Model.IsImpersonating || Model.CanUserSwitch)
            {
                <ul class="dropdown-menu" role="menu">
                    @{if (Model.IsImpersonating)
                        {
                            <li role="presentation"><a role="menuitem" tabindex="-1" id="restoreUser" href="#">Switch back to @Model.RealUserName</a></li>
                        }
                    }
                    @{if (Model.CanUserSwitch)
                        {
                            <li role="presentation"><a id="dscSession" role="menuitem" tabindex="-1" href="">Primary DSC Livy Session</a></li>
                            <li role="presentation"><a role="menuitem" tabindex="-1" href="@Url.Action("Switch", "User")">User Switch to Anybody</a></li>
                            <li class="divider"></li>
                            <li class="dropdown-header">Quick User Switch</li>
                            <li role="presentation"><a role="menuitem" tabindex="-1" href="@Url.Action("RequestSwitch", "User", new { id = "070557" })">Manage Asset Alerts</a></li>
                            <li role="presentation"><a role="menuitem" tabindex="-1" href="@Url.Action("RequestSwitch", "User", new { id = "081082" })">Inside IT, No Access</a></li>
                            <li role="presentation"><a role="menuitem" tabindex="-1" href="@Url.Action("RequestSwitch", "User", new { id = "082728" })">Outside IT, No Access</a></li>
                        }
                    }
                </ul>
            }
        </div>
