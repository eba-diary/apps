                        <li class="dropdown-submenu">
                            <a href="#" data-toggle="dropdown" class="dropdown-toggle">Assets</a>
                            <ul class="dropdown-menu z-depth-1">
                                @foreach (Sentry.data.Web.Models.AssetUIModel da in ViewBag.DataAssets)
                                {
                                    <li class="dropdown-item"><a href="/DataAsset/@da.Name">@da.DisplayName</a></li>
                                }
                            </ul>
                        </li>

                        <li class="dropdown-submenu">
                            <a href="#" data-toggle="dropdown" class="dropdown-toggle">Lineage</a>
                            <ul class="dropdown-menu z-depth-1">
                                <li class="dropdown-item dropdown-submenu">
                                    <a href="#" data-toggle="dropdown" class="dropdown-toggle">Personal Lines</a>
                                    <ul class="dropdown-menu  z-depth-1">
                                        <li><div class="dropdown-header">Search</div></li>
                                        <li class="dropdown-item"><a href="/Lineage/Personal Lines/SERAPL/">Business Term</a></li>
                                        <li><div class="dropdown-header">Assets</div></li>

                                        <li class="dropdown-item dropdown-submenu">
                                            <a href="#" data-toggle="dropdown" class="dropdown-toggle">SERA PL</a>
                                            <ul class="dropdown-menu  z-depth-1">
                                                <li><div class="dropdown-header">Search</div></li>

                                                <li class="dropdown-item"><a href="/Lineage/Personal Lines/SERAPL/">Business Term</a></li>

                                                <li><div class="dropdown-header">Cubes</div></li>

                                                <li class="dropdown-item"><a href="/Lineage/Personal Lines/SERAPL/Cubes/SERAPL Loss Triangles">SERAPL Loss Triangles</a></li>
