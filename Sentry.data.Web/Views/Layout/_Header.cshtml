<div class="sticky-top">
    <nav class="sentry-header navbar navbar-light" style="border: none;">
        <a class="navbar-brand" href="/">
            <img class="sentry-logo" src="~/Images/logo_blue.svg" alt="">
        </a>
        <div class="sentry-app-title">
            <h1>data.sentry.com</h1>
            <p>Do we want something here?</p>
        </div>



        <div class="right-buttons">
            <div class="dropdown sentry-user-dropdown">
                <button class="dropdown-toggle sentry-user btn btn-primary waves-effect waves-light" data-toggle="dropdown">
                    @{ if (Model.IsImpersonating)
                        {
                            <span class="icon-account"></span>

                        }
                        else
                        {
                            <span><img src="@Model.AssociatePhotoUrl" /></span>
                        }

                    }
                    <span class="sentry-user-name">@Model.CurrentUserName</span>
                </button>
                @if (Model.IsImpersonating || Model.CanUserSwitch)
                {
                    <div class="dropdown-menu dropdown-menu-right" role="menu">
                        @{if (Model.IsImpersonating)
                            {
                                <a class="dropdown-item" tabindex="-1" id="restoreUser" href="#">Switch back to @Model.RealUserName</a>
                            }
                        }
                        @{if (Model.CanUserSwitch)
                            {
                                <a id="dscSession" class="dropdown-item" tabindex="-1" href="">Primary DSC Livy Session</a>
                                <a class="dropdown-item" tabindex="-1" href="@Url.Action("Switch", "User")">User Switch to Anybody</a>
                                <div class="dropdown-divider"></div>
                                <div class="dropdown-header">Quick User Switch</div>
                                <a class="dropdown-item" tabindex="-1" href="@Url.Action("RequestSwitch", "User", new { id = "070557" })">Manage Asset Alerts</a>
                                <a class="dropdown-item" tabindex="-1" href="@Url.Action("RequestSwitch", "User", new { id = "081082" })">Inside IT, No Access</a>
                                <a class="dropdown-item" tabindex="-1" href="@Url.Action("RequestSwitch", "User", new { id = "082728" })">Outside IT, No Access</a>
                            }
                        }
                    </div>
                }
            </div>

            @{if (Model.EnvironmentName.ToUpper() != "PROD")
                {
                    <button class="sentry-environment btn btn-danger waves-effect waves-light">
                        <span class="sentry-environment-name">@Model.EnvironmentName.ToUpper()</span>
                    </button>
                }
            }

        </div>
    </nav>

