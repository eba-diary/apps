
@model Dictionary<string, string>
    @{
        ViewBag.Title = "Admin";
        ViewBag.Fluid = true;
    }


<div class="row">
    <div class="card form-wrapper col-lg-2 side-nav-wrapper" id="sideNav">
        <div class="card-body">
            <h2 class="card-title">Admin Page</h2>

            @foreach (var key in Model.Keys)
            {
                 <a href="#" data-url='@Url.Action("GetAdminTest", "Admin", new { viewId = key })' class="load-partial-view"><h4>@Model[key]</h4></a>

            }
        </div>
    </div>
    <div class="col-lg-10" id="colorOrange">
        <center id="partial-view-test"></center>
    </div>
</div>


@Scripts.Render("~/bundles/jquery")

<script>
    console.log("test");

    $(".load-partial-view").click(function (event) {
        event.preventDefault();
        var url = $(this).data("url");
        $("#partial-view-test").load(url);
    });
</script>

<style>
    #sideNav{
        height: 100%;
        position: absolute;
        background-color: lightgray;
    }
</style>