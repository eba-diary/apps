@using Sentry.data.Web;
@model BusinessAreaLandingPageModel
@{
    ViewBag.Title = "Personal Lines";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div id="pl-landing-pg-wrapper">

        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12"><img id="dairyland-logo" alt="Dairyland Logo" src="~/Images/dairyland_logo.png" width="220" /></div>
        </div>

        <div class="row">
            <div sho class="col-sm-12 col-md-12 col-lg-12">

                @* loop through each row in the model *@
                @foreach (BusinessAreaTileRowModel row in Model.Rows)
                {
                    @Html.Partial("_LandingPageTile", row)
                }

            </div>
        </div>
    </div>


@section Scripts {
    <script type="text/javascript">

        $(function () {
            data.BusinessArea.init(1);
        });

    </script>
}