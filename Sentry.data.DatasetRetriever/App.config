<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
		<sectionGroup name="sentrySettings">
			<section name="EnvironmentSettings" type="Sentry.Configuration.EnvironmentSettingsSectionHandler,Sentry.Configuration" />
		</sectionGroup>
	</configSections>
	<startup>
	</startup>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="log4net" publicKeyToken="669e0ddf0bb1aa2a" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-1.2.15.0" newVersion="1.2.15.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="NHibernate" publicKeyToken="aa95f207798dfdb4" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-4.0.0.4000" newVersion="4.0.0.4000" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="MiniProfiler" publicKeyToken="b44f9351044011a3" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-3.2.0.157" newVersion="3.2.0.157" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
	<!-- log4net settings -->
	<log4net threshold="ALL">
		<logger name="Sentry.LoggerDev">
			<level value="DEBUG" />
			<appender-ref ref="RollingLogFileAppenderLocal" />
      <appender-ref ref="SELAppenderLocal" />
		</logger>
		<logger name="Sentry.LoggerTest">
			<level value="DEBUG" />
			<appender-ref ref="RollingLogFileAppenderServer" />
      <appender-ref ref="SELAppenderServer" />
		</logger>
		<logger name="Sentry.LoggerQual">
			<level value="INFO" />
			<appender-ref ref="RollingLogFileAppenderServer" />
      <appender-ref ref="SELAppenderServer" />
		</logger>
		<logger name="Sentry.LoggerProd">
			<level value="INFO" />
			<appender-ref ref="RollingLogFileAppenderServer" />
      <appender-ref ref="SELAppenderServer" />
		</logger>
		<root>
			<level value="OFF" />
		</root>
		<appender name="RollingLogFileAppenderLocal" type="log4net.Appender.RollingFileAppender">
			<file type="log4net.Util.PatternString">
				<conversionPattern value="C:\\Temp\\AppLogs\\Sentry.Sentry.data.DatasetRetriever.ConsoleApp_%property{RequestID}.log" />
			</file>
			<appendToFile value="true" />
			<rollingStyle value="Size" />
			<maxSizeRollBackups value="10" />
			<maximumFileSize value="20MB" />
			<staticLogFileName value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%-23date [%-5thread] %-5level %message%newline" />
			</layout>
		</appender>
    <appender name="SELAppenderLocal" type="Sentry.Common.LogAppenders.NonWebApplicationAppender, Sentry.Common">
      <file value="This is not used" />
      <applicationName value="DatasetRetriever" />
      <logFileName type="log4net.Util.PatternString" value="DatasetRetrieverLogFile_%property{RequestID}.log" />
      <logFolderFilePath value="C:\\Temp\\AppLogs\\DatasetRetriever\\" />
      <!-- Use this environmentName value if you are using Sentry.Configuration and have the 2.1.6 version-->
      <environmentName value="Sentry.Configuration.EnvironmentName, Sentry.Configuration" />
    </appender>
		<appender name="RollingLogFileAppenderServer" type="log4net.Appender.RollingFileAppender">
			<file type="log4net.Util.PatternString">
				<conversionPattern value="D:\\AppLogs\\Sentry.data.DatasetRetriever\\Sentry.Sentry.data.DatasetRetriever.ConsoleApp_%property{RequestID}.log" />
			</file>
			<appendToFile value="true" />
			<rollingStyle value="Size" />
			<maxSizeRollBackups value="10" />
			<maximumFileSize value="20MB" />
			<staticLogFileName value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%-23date [%-5thread] %-5level %message%newline" />
			</layout>
		</appender>
    <appender name="SELAppenderServer" type="Sentry.Common.LogAppenders.NonWebApplicationAppender, Sentry.Common">
      <file value="This is not used" />
      <applicationName value="DatasetRetriever" />
      <logFileName value="DatasetRetrieverLogFile.log" />
      <logFolderFilePath value="D:\\AppLogs\\DatasetRetriever\\" />
      <!-- Use this environmentName value if you are using Sentry.Configuration and have the 2.1.6 version-->
      <environmentName value="Sentry.Configuration.EnvironmentName, Sentry.Configuration" />
    </appender>
	</log4net>
	<sentrySettings>
		<!-- NOTE: Use Ctrl+K Ctrl+D to reformat your config file to make the following readable -->
		<!-- If you have a shared "defaultSettings" config file, import it here -->
		<!-- <importSettings path="defaultSettings.config" /> -->
		<EnvironmentSettings defaultDomain="sentry.com" xmlns="urn:Sentry.Configuration">
      <defaultEnvironmentList>
        <env name="Dev">
          <machine nameRegex="(sho|dix|mad|ste)(is|it).*" executablePathRegex=".*Mainline[^-].*"/>
        </env>
        <env name="NR_D">
          <!--<webHost name="localhost" />-->
          <machine nameRegex="(sho|dix|mad|ste)(is|it).*" executablePathRegex=".*Mainline-NR.*" />
        </env>
        <env name="Test">
          <machine nameRegex="(sho|dix)-t-.*" executablePathRegex=".*[^-][^N][^R]Test\.*" />
        </env>
        <!--<env name="NR_T">
          --><!--<webHost nameRegex="DatasetLoadertest.?" />--><!--
          <machine nameRegex="(sho|dix)-t-.*" executablePathRegex=".*NRTest\.*" />
        </env>-->
        <env name="Qual">
          <machine nameRegex="(sho|dix)-q-.*" />
        </env>
        <env name="Prod">
          <machine nameRegex="(sho|dix)-p-.*" />
        </env>
      </defaultEnvironmentList>
			<config key="EnvironmentName">
				<settingList>
					<setting name="Dev" value="Dev" />
					<setting name="Test" value="Test" />
					<setting name="Qual" value="Qual" />
					<setting name="Prod" value="Prod" />
				</settingList>
			</config>
			<config key="AppLogger">
        <settingList>
          <setting name="Dev" value="Sentry.LoggerDev" />
          <setting name="NR_D" value="Sentry.LoggerDev" />
          <setting name="Test" value="Sentry.LoggerTest" />
          <setting name="NR_T" value="TestNR" />
          <setting name="Qual" value="Sentry.LoggerQual" />
          <setting name="Prod" value="Sentry.LoggerProd" />
        </settingList>
			</config>
      <config key="DatabaseConnectionString">
        <settingList>
          <setting name="Dev" value="Data Source=DIX-D-SHDDBS-02.sentry.com;Initial Catalog=SentryDatasets;Integrated Security=SSPI;" />
          <setting name="NR_D" value="Data Source=DIX-D-SHDDBS-02.sentry.com;Initial Catalog=SentryDatasets_NR;Integrated Security=SSPI;" />
          <setting name="Test" value="Data Source=DIX-T-SHDDBS-02.sentry.com;Initial Catalog=SentryDatasets;Integrated Security=SSPI;" />
          <setting name="NR_T" value="Data Source=DIX-T-SHDDBS-02.sentry.com;Initial Catalog=SentryDatasets_NR;Integrated Security=SSPI;" />
          <setting name="Qual" value="Data Source=DIX-Q-SHDDBS-02.sentry.com;Initial Catalog=SentryDatasets;Integrated Security=SSPI;" />
          <setting name="Prod" value="Data Source=SHO-P-SHDDBS-02.sentry.com;Initial Catalog=SentryDatasets;Integrated Security=SSPI;" />
        </settingList>
      </config>
      <config key="ObsidianUserCacheTimeoutMinutes">
        <settingList>
          <setting name="Dev,NR_D,Test,NR_T,Qual" value="1" />
          <setting name="Prod" value="10" />
        </settingList>
      </config>
      <config key="ObsidianAdServiceUrl">
        <settingList>
          <setting name="Dev,NR_D,Test,NR_T" value="https://ObsidianAdServiceTest.sentry.com/ObsidianAdService.asmx" />
          <setting name="Qual" value="https://ObsidianAdServiceQual.sentry.com/ObsidianAdService.asmx" />
          <setting name="Prod" value="https://ObsidianAdService.sentry.com/ObsidianAdService.asmx" />
        </settingList>
      </config>
      <config key="ShowUserChoice">
        <settingList>
          <setting name="Dev,NR_D,Test,NR_T,Qual" value="true" />
          <setting name="Prod" value="false" />
        </settingList>
      </config>
      <config key="UseNHibernateSQLLogging">
        <settingList>
          <setting name="Dev,NR_D" value="true" />
          <setting name="Test,NR_T,Qual,Prod" value="false" />
        </settingList>
      </config>
      <config key="NOAA_APIKey">
        <settingList>
          <setting name="Dev,NR_D,Test,NR_T,Qual,Prod" value="SJCvXfeARwTJNbbZDiLHSPcjSyeNUtwf" />
        </settingList>
      </config>
      <config key="DatasetLoaderDFSPath">
        <settingList>
          <setting name="Dev,NR_D" value="C:\tmp\DatasetLoader\" />
          <setting name="Test" value="\\sentry.com\appfs_nonprod\DatasetLoader\Test\" />
          <setting name="NR_T" value="\\sentry.com\appfs_nonprod\DatasetLoader_NRTest\" />
          <setting name="Qual" value="\\sentry.com\appfs_nonprod\DatasetLoader\" />
          <setting name="Prod" value="\\sentry.com\appfs\DatasetLoader\" />
        </settingList>
      </config>
      <config key="SentryWebProxyHost">
        <settingList>
          <setting name="Dev,NR_D" value="webproxy.sentry.com" />
          <setting name="Test,NR_T" value="serverproxyqual.sentry.com" />
          <setting name="Qual" value="serverproxyqual.sentry.com" />
          <setting name="Prod" value="serverproxyprod.sentry.com" />
        </settingList>
      </config>
		</EnvironmentSettings>
	</sentrySettings>
  <appSettings>
    <add key="ApplicationName" value="DataLoaderConsoleApp" />
    <!-- ###  BEGIN Sentry.Data  A### - Code below is Sentry.Data-specific -->
    <add key="AuctionCompleterSleepMinutes" value="5" />
    <add key="AWSRegion" value="us-east-1" />
    <add key="SentryWebProxyPort" value="80" />
    <add key="SentryS3ProxyPort" value="80" />
    <!-- ###  END Sentry.Data  ### - Code above is Sentry.Data-specific -->
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
</configuration>
